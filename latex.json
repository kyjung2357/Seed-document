{	// Made by Jung, Kiyuob (email: kyjung2357@gmail.com)
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Equation without numbering": {
		"prefix": "eq",
		"body": [
			"\\begin{equation*}",
			"    $0",
			"\\end{equation*}",
			""
			]
	},

	"Equation with numbering": {
		"prefix": "eqn",
		"body": [
			"\\begin{equation}    \\label{eq: $1}",
			"    $0",
			"\\end{equation}",
			""
			]
	},

	"Align without numbering":{
		"prefix": "ag",
		"body": [
			"\\begin{align*}",
			"    $1",
			"    &${2:=} $3   \\\\\\",
			"    &${4:=} $5   \\\\\\",
			"    &${6:=} $7",
			"\\end{align*}",
			"",
			"$0"
		]
	},

	"Align with numbering":{
		"prefix": "agn",
		"body": [
			"\\begin{align}",
			"    $1",
			"    &${2:=} $3    \\label{ag: $4}    \\\\\\",
			"    &${5:=} $6    \\label{ag: $7}    \\\\\\",
			"    &${8:=} $9    \\label{ag: $10}  ",
			"\\end{align}",
			"",
			"$0"
		]
	},

	"Definition":{
		"prefix": "def",
		"body": [
			"\\begin{definition}    \\label{def: $1}",
			"    $0",
			"\\end{definition}",
			""
		]
	},
	
	"Theorem":{
		"prefix": "thm",
		"body": [
			"\\begin{theorem}    \\label{thm: $1}",
			"    $0",
			"\\end{theorem}",
			""
		]
	},

	"Proposition":{
		"prefix": "prop",
		"body": [
			"\\begin{proposition}    \\label{prop: $1}",
			"    $0",
			"\\end{proposition}",
			""
		]
	},

	"Lemma":{
		"prefix": "lmm",
		"body": [
			"\\begin{lemma}    \\label{lmm: $1}",
			"    $0",
			"\\end{lemma}",
			""
		]
	},

	"Corollary":{
		"prefix": "crl",
		"body": [
			"\\begin{corollary}    \\label{crl: $1}",
			"    $0",
			"\\end{corollary}",
			""
		]
	},

	"proof": {
		"prefix": "pf",
		"body": [
			"\\begin{proof}",
			"    $0",
			"\\end{proof}"
		]
	},

	"And in line":{
		"prefix": "qqand",
		"body": [
			"",
			"\\qquad\\text{${1:and}}\\qquad",
			"$0"
		]
	},

	"Two cases":{
		"prefix": "css2",
		"body": [
			"\\begin{equation*}",
			"    ${1:}",
			"    \\begin{cases}",
			"        $2    &    \\mbox{${3:if} } $4,    \\\\\\",
			"        $5    &    \\mbox{${6:if} } $7      \\\\\\",
			"    \\end{cases}",
			"\\end{equation*}",
			"$0"
		]
	},

	"Three cases":{
		"prefix": "css3",
		"body": [
			"\\begin{equation*}",
			"    ${1:}",
			"    \\begin{cases}",
			"        $2    &    \\mbox{${3:if} } $4,    \\\\\\",
			"        $5    &    \\mbox{${6:if} } $7,    \\\\\\",
			"        $8    &    \\mbox{${9:if} } $10     \\\\\\",
			"    \\end{cases}",
			"\\end{equation*}",
			"$0"
		]
	},

	"Inserting a picture": {
		"prefix": "pct",
		"body": [
			"\\begin{figure}[h]",
			"    \\centering",
			"    \\includegraphics[width=${1:1}\\textwidth]{${2:write the file name}}",
			"    \\caption{$3}",
			"    \\label{fig: $4}",
			"\\end{figure}",
			"",
			"$0"
		]
	},

	"Integration": {
		"prefix": "int",
		"body": "\\int_{$1}^{$2}  $3  \\, d${4:x} $0"
	},

	"Double integration": {
		"prefix": "iint",
		"body": "\\int_{$1}^{$2} \\int_{$3}^{$4}  $5  \\, d${6:x} d${7:y} $0"
	},

	"Displaystyle": {
		"prefix": "dp",
		"body": "\\displaystyle $0"
	},

	"color box": {
		"prefix": "cb",
		"body": "\\colorbox{yellow}{$1}$0" 
	},

	"color text": {
		"prefix": "ct",
		"body": "\\textcolor{green}{$1}$0"
	},

	"absolute value": {
		"prefix": "@1",
		"body": "\\left\\vert $1 \\right\\vert $0" 
	},

	"norm": {
		"prefix": "@11",
		"body": "\\left\\| $1 \\right\\| $0" 
	},
	
	"varepsilon": {
		"prefix": "@3",
		"body": "\\varepsilon $0" 
	},
	
	"sequence": {
		"prefix": "sq",
		"body": "\\left\\lbrace $1 \\right\\rbrace_{$2}^{$3} $0" 
	},

	"Real numbers": {
		"prefix": "Rn",
		"body": "\\mathbb{R}$0" 
	},

	"Complex numbers": {
		"prefix": "Cn",
		"body": "\\mathbb{C}$0" 
	}
}
